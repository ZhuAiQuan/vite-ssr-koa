{
  "name": "vite-vue-ssr-study",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "preview": "vite preview",
    "dev:server": "set NODE_ENV=development&& node server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "start": "set NODE_ENV=production&& node server"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.2",
    "axios": "^1.6.0",
    "koa": "^2.14.2",
    "node-fetch": "^3.3.2",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@vitejs/plugin-vue": "^4.2.3",
    "koa-compress": "^5.1.1",
    "koa-connect": "^2.1.0",
    "koa-static": "^5.0.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
